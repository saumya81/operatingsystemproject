#include<stdio.h>
#include<conio.h>
int main()
{
	printf("enter the number of process");
	scanf("%d",n);
	for(i=0;i<n;i++)
	{
		printf("enter the arrival time ");
		scanf("%d ",at[i]);
		printf("enter the burst time");
		scanf("%d",bt[i]);
		
	}
	printf("enter the quantum number");
	scanf("%d",qn);
	for(i=0;i<qn;i++)
	{
		for(i=0;i<n;i++)
		{
			if(at[i]<at[i+1])
			min=at[i];
		}
	
		
	}
	
}
